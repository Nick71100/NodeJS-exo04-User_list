<main>
  <h1>This is the Sign-In Page</h1>
  <form action="/sign-in" method="POST">
    <input
      type="text"
      id="name"
      name="name"
      placeholder="Nom d'utilisateur..."
      required
    />

    <input
      type="password"
      id="password"
      name="password"
      placeholder="Mot de passe..."
      required
    />

    <button type="submit">Se connecter</button>
  </form>
</main>
